<mvc:View
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:macros="sap.fe.macros"
    xmlns:html="http://www.w3.org/1999/xhtml"
    controllerName="paymentdashboard.ext.main.Main"
>
    <macros:Page id="Page" title="Jobs">
        <macros:FilterBar
            id="JobsFilter"
            metaPath="/Jobs/@com.sap.vocabularies.UI.v1.SelectionFields#filterBarMacro"
            liveMode="true"
        />
        <macros:Table
            id="TableJobs"
            metaPath="/Jobs/@com.sap.vocabularies.UI.v1.LineItem#tableMacro"
            filterBar="JobsFilter"
            header="Jobs"
            isSearchable="true"
            readOnly="true"
        />
    </macros:Page>
</mvc:View>

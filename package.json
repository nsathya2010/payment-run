{
    "name": "payment-run",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@cap-js/hana": "^2",
        "@sap/cds": "^9",
        "@sap/xssec": "^4",
        "cds-numberrange-plugin": "^1.0.1",
        "express": "^4"
    },
    "engines": {
        "node": "^22"
    },
    "devDependencies": {
        "@cap-js/cds-typer": "^0.36",
        "@cap-js/cds-types": "^0.14.0",
        "@cap-js/sqlite": "^2",
        "@prettier/plugin-xml": "^3.4.2",
        "@sap/cds-dk": "^9",
        "@sap/eslint-plugin-cds": "^4",
        "eslint": "^9",
        "husky": "^9.1.7",
        "kill-port": "^2.0.1",
        "ncu": "^0.2.1",
        "prettier": "^3.6.2"
    },
    "scripts": {
        "start": "cds-serve"
    },
    "cds": {
        "requires": {
            "auth": {
                "[hybrid]": {
                    "kind": "dummy"
                }
            }
        }
    },
    "imports": {
        "#cds-models/*": "./@cds-models/*/index.js"
    },
    "cds-numberrange-plugin": {
        "ranges": [
            {
                "name": "JobNumber",
                "start": 1,
                "increment": 1,
                "createOnDraft": true,
                "additionalProperties": {
                    "prefix": "JOB",
                    "padCount": 9,
                    "padValue": "0"
                }
            }
        ]
    }
}
